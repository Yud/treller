{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "no-console": 0,
      "comma-dangle": ["error", "never"],
      "no-underscore-dangle": "allow"
    },
     "globals": {
        "describe": true,
        "it": true
    }
}
